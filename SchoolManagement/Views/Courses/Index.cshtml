
@{
    ViewBag.Title = "Index";
}

<div class="slim-mainpanel">
    <div class="container">
        <div class="slim-pageheader">
            <ol class="breadcrumb slim-breadcrumb">
                <li class="breadcrumb-item active" aria-current="page"> @Html.ActionLink("Add Account", "Index", "Adminsion", new { id = "100" }, null)</li>
                <li class="breadcrumb-item">@Html.ActionLink("Import File Excel", "ImportFile", "Adminsion", new { id = "200" }, null) </li>
            </ol>
            <h6 class="slim-pagetitle">New Study-program</h6>
        </div><!-- slim-pageheader -->
    </div>
    <div class="section-wrapper">
        <div class="row mg-b-20">
            <div class="col-lg-4">
                <div class="form-group">
                    <label class="form-control-label">Courses name: <span class="tx-danger">*Required</span></label>
                    <input class="form-control" type="text" name="firstname" placeholder="Input new Course name">
                </div>
            </div><!-- col-4 -->
            <div class="col-lg-4">
                <div class="form-group">
                    <label class="form-control-label">
                        Select Branch: <span class="tx-danger">*</span>
                    </label>
                    <select class="form-control select2" data-placeholder="Choose one" data-parsley-class-handler="#slWrapper" data-parsley-errors-container="#slErrorContainer" required>
                        <option value="APTECH HA NOI">Aptech</option>
                    </select>
                </div>
            </div><!-- col-4 -->

        </div>
        <div class="row row-xs">
            <div class="col-sm-6 mb-2">
                <div class="card card-status">
                    <div class="media">
                        <div class="media-body">
                            <h4>Semester 1</h4>
                            <button data-toggle="modal" data-target="#modaldemo8" type="submit" class=" btn-primary" style="float: right; border-radius: 5px;">Add Subject</button>
                        </div><!-- media-body -->
                    </div><!-- media -->
                </div><!-- card -->
            </div><!-- col-3 -->
            <div class="col-md-6">
                <div class="card card-status">
                    <div class="media">
                        <div class="media-body">
                            <h4>Semester 2</h4>
                            <button data-toggle="modal" data-target="#modaldemo2" type="submit" class=" btn-primary" style="float: right; border-radius: 5px;">Add Subject</button>
                        </div><!-- media-body -->
                    </div><!-- media -->
                </div><!-- card -->
            </div><!-- col-3 -->
            <div class="col-md-6">
                <div class="card card-status">
                    <div class="media">
                        <div class="media-body">
                            <h4>Semester 3</h4>
                            <button data-toggle="modal" data-target="#modaldemo4" type="submit" class=" btn-primary" style="float: right; border-radius: 5px;">Add Subject</button>
                        </div><!-- media-body -->
                    </div><!-- media -->
                </div><!-- card -->
            </div><!-- col-3 -->
            <div class="col-md-6">
                <div class="card card-status">
                    <div class="media">
                        <div class="media-body">
                            <h4>Semester 4</h4>
                            <button data-toggle="modal" data-target="#modaldemo5" type="submit" class=" btn-primary" style="float: right; border-radius: 5px;">Add Subject</button>
                        </div><!-- media-body -->
                    </div><!-- media -->
                </div><!-- card -->
            </div><!-- col-3 -->
            <div class="col-md-12">
                <div class="form-group">
                    <button type="submit" class="btn btn-primary pd-x-30" style="margin-top: 30px; background-color: #868ba1; border: 1px solid #868ba1;">Close</button>
                    <button type="submit" class="btn btn-primary pd-x-30" style="float: right; margin-top: 30px;">Create</button>
                </div>
            </div><!-- col-4 -->
        </div><!-- row -->
    </div>
</div>






















<div id="modaldemo8" class="modal fade">
    <div class="modal-dialog modal-dialog-vertical-center" role="document">
        <div class="modal-content bd-0 tx-14">
            <div class="modal-header pd-y-20 pd-x-25">
                <h6 class="tx-14 mg-b-0 tx-uppercase tx-inverse tx-bold">Message Preview</h6>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body pd-25">
                <h5 class="lh-3 mg-b-20 tx-inverse">Why We Use Electoral College, Not Popular Vote</h5>
                <p class="mg-b-5">It is a long established fact that a reader will be distracted by the readable content of a page when looking at its layout. The point of using Lorem Ipsum is that it has a more-or-less normal distribution of letters, as opposed to using 'Content here, content here', making it look like readable English. </p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary">Save changes</button>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div><!-- modal-dialog -->
</div><!-- modal -->




